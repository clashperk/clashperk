{
  "name": "clashperk",
  "version": "3.6.13",
  "author": "",
  "license": "UNLICENSED",
  "description": "Clash of Clans Discord Bot",
  "type": "module",
  "main": "./dist/src/index.js",
  "exports": {
    "main": "./dist/src/index.js"
  },
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc && tsc-alias",
    "postbuild": "node -r dotenv/config dist/locales/export.js",
    "start": "npm run build && node -r dotenv/config --enable-source-maps dist/src/index.js",
    "test": "eslint --ext .ts --ignore-path .gitignore .",
    "lint": "eslint --fix --ext .ts --ignore-path .gitignore .",
    "dev": "node -r dotenv/config --enable-source-maps dist/src/index.js",
    "crowdin": "tsx -r dotenv/config locales/crowdin.ts",
    "deploy": "tsx -r dotenv/config scripts/deploy-script.ts",
    "patrons": "tsx -r dotenv/config scripts/patreon-status-script.ts",
    "pull": "git submodule init && git submodule update --remote",
    "task:index": "tsx -r dotenv/config scripts/entry-maker-script.ts",
    "task:rename": "tsx -r dotenv/config scripts/rename-files.ts",
    "build:sentry": "sentry-cli sourcemaps inject --org suvajit --project clashperk_bot ./dist && sentry-cli sourcemaps upload --org suvajit --project clashperk_bot ./dist --release=$(git rev-parse HEAD)"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.6.0",
    "@google-cloud/local-auth": "^2.1.0",
    "@google-cloud/translate": "^8.3.0",
    "@sapphire/result": "^2.6.4",
    "@sentry/integrations": "^7.51.2",
    "@sentry/node": "^7.51.2",
    "chalk": "^4.1.2",
    "clashofclans.js": "^3.3.12",
    "common-tags": "^1.8.2",
    "discord.js": "^14.14.1",
    "googleapis": "^105.0.0",
    "i18next": "^21.6.13",
    "jsonwebtoken": "^9.0.0",
    "kafkajs": "^2.2.4",
    "mixpanel": "^0.17.0",
    "moment": "^2.29.1",
    "moment-duration-format": "^2.3.2",
    "moment-timezone": "^0.5.43",
    "mongodb": "^6.8.0",
    "ms": "^2.1.3",
    "nanoid": "^4.0.0",
    "node-fetch": "^3.2.10",
    "posthog-node": "^4.0.1",
    "radash": "^11.0.0",
    "readdirp": "^3.6.0",
    "redis": "^4.6.14",
    "reflect-metadata": "^0.1.13",
    "timeout-signal": "^1.1.0",
    "tsyringe": "^4.6.0",
    "undici": "^6.9.0"
  },
  "devDependencies": {
    "@sentry/cli": "^2.32.1",
    "@types/common-tags": "^1.8.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/moment-duration-format": "^2.2.3",
    "@types/ms": "^0.7.31",
    "@types/node": "^20.14.8",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.7",
    "erlpack": "^0.1.4",
    "utf-8-validate": "^6.0.3",
    "zlib-sync": "^0.1.8"
  },
  "engines": {
    "node": ">=18.x"
  },
  "overrides": {}
}