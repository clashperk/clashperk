{
  "name": "clashperk",
  "version": "4.0.1",
  "author": "",
  "license": "MIT",
  "description": "Clash of Clans Discord Bot",
  "type": "module",
  "main": "./dist/src/index.js",
  "exports": {
    "main": "./dist/src/index.js"
  },
  "private": true,
  "scripts": {
    "start": "npm run build && node -r dotenv/config dist/src/index.js",
    "dev": "node -r dotenv/config dist/src/index.js",
    "release": "./scripts/release.sh",
    "prebuild": "rimraf dist",
    "build": "tsc && tsc-alias",
    "postbuild": "node -r dotenv/config dist/scripts/export_locale_script.js",
    "test": "eslint .",
    "diff": "./scripts/git_diff_link.sh",
    "lint": "eslint --fix .",
    "deploy": "tsx -r dotenv/config scripts/update_commands_script.ts",
    "crowdin": "tsx -r dotenv/config scripts/update_crowdin_script.ts",
    "check:locales": "tsx -r dotenv/config scripts/check_duplicate_locales.ts",
    "check:patreon": "tsx -r dotenv/config scripts/check_patreon_status_script.ts",
    "submodule": "git submodule init && git submodule update --remote",
    "knip": "knip"
  },
  "dependencies": {
    "@clickhouse/client": "^1.12.0",
    "@elastic/elasticsearch": "^8.6.0",
    "@google-cloud/translate": "^8.5.1",
    "@googleapis/drive": "^8.16.0",
    "@googleapis/oauth2": "^1.0.7",
    "@googleapis/sheets": "^9.4.0",
    "@sentry/node": "^9.31.0",
    "chalk": "^5.6.2",
    "clashofclans.js": "^3.3.24",
    "discord.js": "^14.21.0",
    "google-auth-library": "^9.15.0",
    "i18next": "^21.6.13",
    "jsonwebtoken": "^9.0.0",
    "mixpanel": "^0.18.1",
    "moment": "^2.29.1",
    "moment-duration-format": "^2.3.2",
    "moment-timezone": "^0.6.0",
    "mongodb": "^6.14.2",
    "ms": "^2.1.3",
    "nanoid": "^5.1.5",
    "pluralize": "^8.0.0",
    "posthog-node": "^4.7.0",
    "radash": "^12.1.0",
    "readdirp": "^4.1.2",
    "redis": "^4.6.14",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/moment-duration-format": "^2.2.3",
    "@types/ms": "^0.7.31",
    "@types/node": "^22.13.10",
    "@types/pluralize": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.32.1",
    "dotenv": "^17.2.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "knip": "^5.58.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.11",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9",
    "erlpack": "^0.1.4",
    "utf-8-validate": "^6.0.5",
    "zlib-sync": "^0.1.8"
  },
  "engines": {
    "node": ">=20.x"
  },
  "overrides": {}
}