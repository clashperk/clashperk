{
	"name": "clashperk",
	"version": "3.1.0",
	"author": "SUVAJIT (suvajit.me@gmail.com)",
	"license": "MIT",
	"description": "Clash of Clans Discord Bot",
	"type": "module",
	"main": "./dist/src/index.js",
	"exports": {
		"main": "./dist/src/index.js"
	},
	"private": true,
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "tsc",
		"postbuild": "node -r dotenv/config dist/locales/export.js",
		"crowdin": "ts-node-esm -r dotenv/config locales/crowdin.ts",
		"start": "npm run build && node -r dotenv/config --enable-source-maps dist/src/index.js",
		"test": "eslint --ext .ts --ignore-path .gitignore .",
		"lint": "eslint --fix --ext .ts --ignore-path .gitignore .",
		"dev": "node -r dotenv/config --enable-source-maps dist/src/index.js",
		"deploy": "ts-node-esm -r dotenv/config scripts/index.ts",
		"patrons": "ts-node-esm -r dotenv/config scripts/Patrons.ts",
		"pull": "git submodule init && git submodule update --remote"
	},
	"dependencies": {
		"@elastic/elasticsearch": "^8.6.0",
		"@google-cloud/local-auth": "^2.1.0",
		"@sentry/integrations": "^7.51.2",
		"@sentry/node": "^7.51.2",
		"@typescript-eslint/typescript-estree": "^5.59.1",
		"chalk": "^4.1.2",
		"clashofclans.js": "^1.5.5",
		"common-tags": "^1.8.2",
		"discord.js": "^14.11.0",
		"googleapis": "^105.0.0",
		"i18next": "^21.6.13",
		"jsonwebtoken": "^9.0.0",
		"mixpanel": "^0.17.0",
		"moment": "^2.29.1",
		"moment-duration-format": "^2.3.2",
		"moment-timezone": "^0.5.43",
		"mongodb": "^5.3.0",
		"ms": "^2.1.3",
		"nanoid": "^4.0.0",
		"node-fetch": "^3.2.10",
		"readdirp": "^3.6.0",
		"redis": "^4.1.0",
		"reflect-metadata": "^0.1.13",
		"timeout-signal": "^1.1.0",
		"tsyringe": "^4.6.0"
	},
	"devDependencies": {
		"@types/common-tags": "^1.8.1",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/moment-duration-format": "^2.2.3",
		"@types/ms": "^0.7.31",
		"@types/node": "^18.16.2",
		"@typescript-eslint/eslint-plugin": "^5.59.1",
		"@typescript-eslint/parser": "^5.59.1",
		"dotenv": "^16.0.2",
		"eslint": "^8.39.0",
		"eslint-config-marine": "^9.0.6",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"prettier": "^2.8.8",
		"rimraf": "^3.0.2",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4"
	},
	"optionalDependencies": {
		"bufferutil": "^4.0.7",
		"erlpack": "^0.1.4",
		"utf-8-validate": "^6.0.3",
		"zlib-sync": "^0.1.8"
	},
	"engines": {
		"node": ">=16.x"
	}
}
